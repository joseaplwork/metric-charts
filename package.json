    "babel-jest": "^24.9.0",
    "babel-loader": "^8.3.0",
    "cors": "^2.8.5",
    "css-loader": "^6.7.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
