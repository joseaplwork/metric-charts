{
  "name": "metrics-chart",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --verbose --watchAll --coverage",
    "start": "node server & APP_ENVIRONMENT=development webpack-dev-server --client-log-level trace",
    "start:prod": "node server & npm run build && cp ./favicon.ico ./dist && serve dist",
    "build": "rimraf ./dist && APP_ENVIRONMENT=production npx webpack --config webpack.config.js"
  },
  "author": "joseaplwork",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "d3": "^5.14.2",
    "lit-html": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cors": "^2.8.5",
    "css-loader": "^3.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "serve": "^11.2.0",
    "style-loader": "^1.1.1",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!lit-html).+\\.js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "^@app(.*)$": "<rootDir>/client$1"
    }
  }
}
